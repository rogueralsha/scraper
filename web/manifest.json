{
  "manifest_version": 2,
  "name": "Scraper",
  "description": "Provides various tools to assist with scraping media from sites",
  "version": "0.1",
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "http://*.artstation.com/*",
        "https://*.artstation.com/*",
        "http://*.comicartcommunity.com/*",
        "https://*.comicartcommunity.com/*",
        "http://*.comicartfans.com/*",
        "https://*.comicartfans.com/*",
        "http://*.deviantart.com/*",
        "https://*.deviantart.com/*",
        "http://*.hentai-foundry.com/*",
        "https://*.hentai-foundry.com/*",
        "http://*.instagram.com/*",
        "https://*.instagram.com/*",
        "http://*.blogspot.com/*",
        "https://*.blogspot.com/*",
        "http://*.gfycat.com/*",
        "https://*.gfycat.com/*",
        "http://*.paheal.net/*",
        "https://*.paheal.net/*",
        "http://*.reddit.com/*",
        "https://*.reddit.com/*",
        "http://*.imgur.com/*",
        "https://*.imgur.com/*",
        "http://*.redd.it/*",
        "https://*.redd.it/*"
      ],
      "js": [
        "content.dart.js"
      ],
      "css": ["content.css"],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Dart Scraper"
  },
  "background": {
    "scripts": [
      "event_page.dart.js"
    ]
  },
  "web_accessible_resources":[
    "packages/*",
    "*.dart",
    "*.js.map",
    "*.js.deps"
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}